			"./base.css",
			"./github-markdown.css",
			"./node_modules/highlight.js/styles/github.css"
		]
	},
	"dependencies": {
		"highlight.js": "^10.4.1"
	},
	"devDependencies": {
		"github-markdown-css": "^3.0.1"
	},
	"scripts": {
