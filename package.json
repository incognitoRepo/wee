{
	"name": "wee",
	"displayName": "wee",
	"description": "eww",
	"version": "0.0.1",
	"publisher": "pubber",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"contributes": {
		"markdown.previewStyles": [
			"./base.css",
			"./github-markdown.css",
			"./node_modules/highlight.js/styles/github.css"
